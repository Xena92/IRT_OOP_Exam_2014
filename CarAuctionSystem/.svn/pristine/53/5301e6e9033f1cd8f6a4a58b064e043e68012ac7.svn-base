        public string CardType()
        {
            set
            {
                if (pullHook)
                {
                    this.CardType = "CE";
                }
                else
                {
                    this.CardType = "C";
                }
            }
            get
            {
                return this.CardType;
            }
            
        }
        public double cargoCapacity
        {
            set { CargoCapacity = value; }
            get { return cargoCapacity;  }
        }

        public double Hight
        {
            set { Hight = value; }
            get { return Hight;  }
        }

        public double Weight
        {
            set { Weight = value; }
            get { return Weight;  }
        }

        public double Lenght
        {
            set { Lenght = value; }
            get { return Lenght;  }
        }
    }
}
